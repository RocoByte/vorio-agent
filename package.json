{
  "name": "vorio-agent",
  "version": "1.0.0",
  "description": "Vorio Agent - Connect your WiFi controller to Vorio Cloud",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "vorio",
    "unifi",
    "mikrotik",
    "voucher",
    "hotspot",
    "sync",
    "wifi",
    "agent"
  ],
  "author": "RocoByte",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RocoByte/vorio-agent.git"
  },
  "bugs": {
    "url": "https://github.com/RocoByte/vorio-agent/issues"
  },
  "homepage": "https://github.com/RocoByte/vorio-agent#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
